  "scripts": {
    "dev": "next dev",
    "build": "NODE_OPTIONS='--max-old-space-size=3072' npx --no-install prisma generate --schema=./prisma/schema.prisma && node scripts/generate-sw-version.js && NODE_OPTIONS='--max-old-space-size=6144' next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "postinstall": "npx --no-install prisma generate --schema=./prisma/schema.prisma || echo 'Prisma generate deferred to build step'",
    "vercel-build": "npx --no-install prisma generate --schema=./prisma/schema.prisma && node scripts/generate-sw-version.js && next build",
    "email": "email dev --dir ./components/emails --port 3001",
    "stripe:webhook": "pkgx stripe listen --forward-to localhost:3000/api/stripe/webhook",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,mdx}\"",
    "dev:prisma": "npx prisma generate && npx prisma migrate deploy",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:sign": "jest __tests__/api/sign",
    "test:auth": "jest __tests__/api/auth",
    "test:datarooms": "jest __tests__/api/datarooms",
    "test:integration": "jest __tests__/integration",
    "test:api": "jest __tests__/api",
    "test:lib": "jest __tests__/lib",
    "test:ee": "jest __tests__/ee",
    "test:funds": "jest __tests__/lib/funds",
    "test:signature": "jest __tests__/lib/signature",
    "test:middleware": "jest __tests__/lib/middleware",
    "test:providers": "jest __tests__/lib/providers",
    "test:storage": "jest __tests__/lib/storage",
    "test:limits": "jest __tests__/ee/limits",
    "test:transactions": "jest __tests__/api/transactions",
    "test:fund-settings": "jest __tests__/api/funds",
    "test:access": "jest __tests__/lib/access",
    "test:credentials": "jest __tests__/lib/organization",
    "test:webhooks": "jest __tests__/lib/webhook",
    "test:roles": "jest __tests__/lib/team",
    "test:ci": "jest --coverage --passWithNoTests --forceExit",
    "test:visual": "npx playwright test",
    "test:visual:update": "npx playwright test --update-snapshots",
    "test:visual:report": "npx playwright show-report e2e/html-report",
    "generate:encryption-key": "node scripts/generate-encryption-key.js"
  },
